{
    "name": "ra-language-german-ts",
    "version": "1.0.0",
    "description": "German messages for react-admin with TS support",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/marmelab/react-admin.git"
    },
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/cjs/index.d.ts",
    "scripts": {
        "build": "yarn run build-cjs && yarn run build-esm",
        "build-cjs": "rimraf ./dist/cjs && tsc --outDir dist/cjs",
        "build-esm": "rimraf ./dist/esm && tsc --outDir dist/esm --module es2015",
        "watch": "tsc --outDir dist/esm --module es2015 --watch"
    },
    "dependencies": {
        "ra-core": "^4.1.5"
    },
    "devDependencies": {
        "rimraf": "^3.0.2",
        "typescript": "^4.4.0"
    },
    "keywords": [
        "react",
        "react-admin",
        "admin-on-rest",
        "translation"
    ],
    "author": "Mertalp Ulus",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/marmelab/react-admin/issues"
    },
    "homepage": "https://github.com/marmelab/react-admin#readme"
}
